{
  "name": "nx-demo-jest",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:app1": "npm start -- --app=app1 --port 4201 --open",
    "build": "ng build",
    "build:app1": "npm run build -- --prod --app=app1",
    "build:app1:server": "npm run build -- --prod --app=app1-ngu-app-shell --output-hashing=false",
    "build:app1:client-and-server-bundles": "npm run build:app1 && npm run build:app1:server",
    "webpack:app1:server": "webpack --config apps/app1/webpack.server.config.js --progress --colors",
    "generate:app1:prerender": "cd dist/apps/app1 && node prerender",
    "build:app1:prerender": "npm run build:app1:client-and-server-bundles && npm run webpack:app1:server && npm run generate:app1:prerender",
    "serve:app1:prerender": "cd dist/apps/app1/browser && http-server",
    "build:app1:ssr": "npm run build:app1:client-and-server-bundles && npm run webpack:app1:server",
    "serve:app1:ssr": "node dist/apps/app1/server",
    "build-serve:app1:ssr": "npm run build:app1:ssr && npm run serve:app1:ssr",
    "build-serve:app1:ssr-prerender": "npm run build:app1:prerender && npm run serve:app1:ssr",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format": "./node_modules/.bin/nx format write",
    "nx-migrate": "./node_modules/.bin/nx migrate",
    "jest": "jest",
    "apps:affected": "./node_modules/.bin/nx affected apps",
    "build:affected": "./node_modules/.bin/nx affected build",
    "e2e:affected": "./node_modules/.bin/nx affected e2e",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "affected:apps": "./node_modules/.bin/nx affected apps",
    "affected:build": "./node_modules/.bin/nx affected build",
    "affected:e2e": "./node_modules/.bin/nx affected e2e",
    "format:write": "./node_modules/.bin/nx format write",
    "format:check": "./node_modules/.bin/nx format check",
    "dyson": "dyson ./dyson",
    "dyson:watch": "nodemon --watch dyson --exec dyson dyson"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.2.1",
    "@angular/common": "5.2.1",
    "@angular/compiler": "5.2.1",
    "@angular/core": "5.2.1",
    "@angular/forms": "5.2.1",
    "@angular/http": "5.2.1",
    "@angular/platform-browser": "5.2.1",
    "@angular/platform-browser-dynamic": "5.2.1",
    "@angular/router": "5.2.1",
    "@ngrx/effects": "5.0.0",
    "@ngrx/router-store": "5.0.0",
    "@ngrx/store": "5.0.0",
    "@ngrx/store-devtools": "5.0.0",
    "@nguniversal/express-engine": "5.0.0-beta.5",
    "@nguniversal/module-map-ngfactory-loader": "5.0.0-beta.5",
    "@nrwl/nx": "0.6.19",
    "bootstrap": "4.0.0",
    "core-js": "2.5.3",
    "jquery": "3.3.1",
    "loglevel": "1.6.1",
    "luxon": "^0.3.1",
    "rxjs": "5.5.6",
    "zone.js": "0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "nrwl/fix-cli-build#release16",
    "@angular/compiler-cli": "5.2.1",
    "@angular/language-service": "5.2.1",
    "@angular/platform-server": "5.2.1",
    "@nrwl/schematics": "0.6.19",
    "@storybook/addon-actions": "3.3.10",
    "@storybook/addon-links": "3.3.10",
    "@storybook/addon-notes": "3.3.10",
    "@storybook/angular": "3.3.10",
    "@types/jest": "22.1.0",
    "@types/jquery": "3.3.0",
    "@types/lodash-es": "4.17.0",
    "@types/node": "9.3.0",
    "codelyzer": "4.1.0",
    "copy-webpack-plugin": "4.3.1",
    "cpy-cli": "1.0.1",
    "dyson": "^2.0.0",
    "dyson-generators": "^0.2.0",
    "express": "4.16.2",
    "faker": "^4.1.0",
    "http-server": "0.11.1",
    "jasmine-core": "2.9.1",
    "jasmine-spec-reporter": "4.2.1",
    "jest": "22.1.4",
    "jest-cli": "22.1.4",
    "jest-html-reporter": "0.6.0",
    "jest-preset-angular": "5.0.0",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.4.0",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "ngrx-store-freeze": "0.2.0",
    "nodemon": "^1.14.10",
    "prettier": "1.10.2",
    "protractor": "5.2.2",
    "raw-loader": "0.5.1",
    "reflect-metadata": "0.1.12",
    "rxjs-marbles": "2.3.0",
    "sass-loader": "6.0.6",
    "ts-loader": "3.3.1",
    "ts-node": "4.1.0",
    "tslint": "5.9.1",
    "tslint-angular": "1.1.0",
    "typescript": "2.6.2",
    "@angular-devkit/core": "^0.0.28"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/jest/setupJest.ts",
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.spec.json"
      },
      "__TRANSFORM_HTML__": true,
      "testResultsProcessor": "./node_modules/jest-html-reporter"
    },
    "collectCoverageFrom": [
      "{apps|libs}/**/src/**/*.ts",
      "!jest/*.ts",
      "!{apps|libs}/**/src/**/*module.ts",
      "!apps/**/src/environments/*.ts",
      "!apps/**/src/main.ts",
      "!apps/**/src/polyfills.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": -20
      }
    },
    "moduleNameMapper": {
      "@nx-demo-jest/(.*)": "<rootDir>/libs/$1"
    }
  },
  "jest-html-reporter": {
    "pageTitle": "Your test suite",
    "outputPath": "coverage/index.html",
    "includeFailureMsg": false
  }
}